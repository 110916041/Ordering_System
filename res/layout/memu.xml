            </LinearLayout>

            <View
                android:id="@+id/vie12"
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#EAA24A" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#F3F0C6"/>

        </LinearLayout>


    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>